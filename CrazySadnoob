		RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(100), math.rad(0), math.rad(0)), 0.3)
	end
	Effects.Block.Create(BrickColor.new("Dark orange"), Cylinder1.CFrame, 2, 2, 2, 0.9, 0.9, 0.9, 0.05)
    	Effects.Block.Create(BrickColor.new("Really red"), Cylinder1.CFrame, 2, 2, 2, 0.5, 0.5, 0.5, 0.05)
    	CreateSound("249050335", char, 1, .5)
