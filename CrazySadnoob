VALUE1 = false


coroutine.wrap(function()
	while true do
		swait(15)
		hum.Health = hum.Health + 55
		local chance = 200
		local name = math.random(1,45)
		if(name == 1)then
			tecks2.Text = "iNSaNiTY"
			wait(.3)
			tecks2.Text = "Insane Genocidal Maniac"
		elseif(name == 2)then
			tecks2.Text = "S stands for ;)"
			wait(.3)
			tecks2.Text = "Insane Genocidal Maniac"
		elseif(name == 3)then
			tecks2.Text = ":))))))))))))))))))"
			wait(.3)
			tecks2.Text = "Insane Genocidal Maniac"
		elseif(name == 4) then
		    tecks2.Text = "WORTTTThLESS"
		    wait(.3)
			tecks2.Text = "Insane Genocidal Maniac"
		elseif(math.random(1,chance) == math.ceil(chance/2))then
			tecks2.Text = "Why BOOOOTHer"
		else
			tecks2.Text = "Insane Genocidal Maniac"
		end
		if(VALUE1)then
			tecks2.Text = tecks2.Text:gsub("",string.char(math.random(1,127)))
		end
	end
end)()

