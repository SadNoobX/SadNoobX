mouse.Button1Down:connect(function()
	if attack == false and targetted ~= nil and targetted.Parent ~= nil then
		targetkill()
	end
end)

mouse.KeyDown:connect(function(key)
	if attack == false then
	if key == 't' then
		Laugh()
	elseif key == 'v' then
		smileforme()
	elseif key == 'q' then
	    LockOn()
	elseif key == 'f' and targetted ~= nil then
		frombelow()
	elseif key == 'x' then
		shootyboi()
	elseif key == 'e' and targetted ~= nil then
		nothinpersonal(targetted.Head.CFrame * CFrame.new(0,-1.5,3))
	--[[elseif key == 'z' then
	waitwathowdididashwithjustmyhumanoidokaynvm()]]--
	elseif key == 'b' and angery == false then
		ohshitimangry()
	--[[elseif key == 'p' then
		suicide()]]--
	end
	end
end)





